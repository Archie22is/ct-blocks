{
	"apiVersion": 2,
	"name": "ct-blocks/image-slider",
	"version": "0.0.1",
	"title": "Image Slider",
	"category": "ct-blocks",
	"icon": "smiley",
	"keywords": ["image", "slider", "slideshow"],
	"supports": {
		"html": false
	},
	"textdomain": "ct-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"images": {
			"type": "array",
			"default": []
		},
		"enableAutoplay": {
			"type": "boolean",
			"default": false
		},
		"autoplayTiming": {
			"type": "number",
			"default": "3"
		},
		"pageDots": {
			"type": "boolean",
			"default": true
		},
		"prevNextButtons": {
			"type": "boolean",
			"default": "true"
		},
		"adaptiveHeight": {
			"type": "boolean",
			"default": "true"
		},
		"wrapAround": {
			"type": "boolean",
			"default": "true"
		},
		"cellAlign": {
			"type": "string",
			"default": "left"
		}
	}
}
